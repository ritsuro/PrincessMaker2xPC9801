AS	=	ML
ASFLAGS	=	/VM /I. /Zm /c /Fo$@

DSKTOOL.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

MAINLP.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

WIDDATSL.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

WIDCMND.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

WIDUSER.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

DSKCHECK.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

JAPAN.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

MNGATE.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

MACLIB.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

DKFORM.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

SCWRT.OBJ	:	$*.ASM
	$(AS) $(ASFLAGS) $*.ASM

DSKTOOL.COM	:	DSKTOOL.OBJ MAINLP.OBJ WIDDATSL.OBJ WIDCMND.OBJ WIDUSER.OBJ DSKCHECK.OBJ JAPAN.OBJ MNGATE.OBJ MACLIB.OBJ DKFORM.OBJ SCWRT.OBJ
	OPTLINK @LINKFILE

..\NDOC\DSKTOOL.COM	:	$*.COM
	COPY $*.COM $@

