;
;	machine flag.
;
; subroutine.

MFLAG_PCAT	EQU	1		; PC/AT
MFLAG_PC98	EQU	2		; PC9801

DATA	SEGMENT	PUBLIC	'DATA'

EXTRN	MFLAG_MY:WORD			; machine flag.

DATA	ENDS


MACHINE_CALL	MACRO	PCAT,PC98
	LOCAL	L1
	MOV	BP,MFLAG_MY		; machine flag.
	DEC	BP
	AND	BP,1
	SHL	BP,1
	CALL	WORD PTR DS:L1[BP]
DATA	SEGMENT	PUBLIC	'DATA'
L1	DW	PCAT,PC98
DATA	ENDS
	ENDM

;
;	end of "MACHINEX.INC"
;
